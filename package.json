{
  "name": "twitch-chatbot",
  "type": "module",
  "@std/esm": "js",
  "version": "0.0.1",
  "description": "A simple app chatbot example. Using the command !echo will simply repeat the message.",
  "main": "bot.js",
  "test-unit": "NODE_ENV=test mocha '/*.spec.js'",
  "scripts": {
    "start": "npm run prod",
    "server": "NODE_ENV=development @babel-node src/bot_rewrite.js",
    "server:prod": "NODE_ENV=production node ./dist/bot_rewrite.js",
    "dev": "NODE_ENV=development npm-run-all server",
    "clean": "rimraf dist",
    "build": "babel ./src --out-dir dist",
    "prod": "NODE_ENV=production npm-run-all clean build server:prod",
    "watch": "nodemon",
    "rew": "node src/bot_rewrite.js",
    "test-unit": "NODE_ENV=test nyc mocha --recursive -r @babel/register --require co-mocha 'test/' "
  },
  "dependencies": {
    "@std/esm": "^0.26.0",
    "braces": ">=3.0.2",
    "chai": "^4.2.0",
    "core-js": "^3.6.5",
    "dotenv": "^8.2.0",
    "elo-rank": "^1.0.4",
    "express": "^4.17.1",
    "google-spreadsheet": "^3.3.0",
    "list.js": "^1.5.0",
    "mocha": "^6.2.3",
    "npm-run-all": "^4.1.5",
    "pg": "^7.18.2",
    "random-org": "^2.1.0",
    "regenerator-runtime": "^0.13.5",
    "request": "latest",
    "rimraf": "^3.0.2",
    "sequelize": "5.21.6",
    "sequelize-cli": "^5.5.1",
    "sequelize-heroku": "^2.1.5",
    "sinon": "^8.1.1",
    "sqlite3": "^4.2.0",
    "tmi.js": "^1.5.0"
  },
  "engines": {
    "node": "12.13.x",
    "npm": "6.13.x"
  },
  "repository": {
    "url": "https://github.com/JettJengaGod/EloBot"
  },
  "license": "MIT",
  "keywords": [
    "twitch",
    "chatbots",
    "irc",
    "elo",
    "king of the hill"
  ],
  "devDependencies": {
    "@babel/cli": "^7.10.3",
    "@babel/core": "^7.10.3",
    "@babel/node": "^7.10.3",
    "@babel/polyfill": "^7.10.1",
    "@babel/preset-env": "^7.10.3",
    "@babel/register": "^7.10.3",
    "child-process-promise": "^2.2.1",
    "co-mocha": "^1.2.2",
    "esm": "^3.2.25",
    "faker": "^4.1.0",
    "lodash": "latest",
    "nodemon": "^2.0.4",
    "nyc": "^15.1.0",
    "proxyquire": "^2.1.3",
    "require-directory": "latest",
    "sequelize-test-helpers": "^1.2.2",
    "sinon-chai": "^3.5.0"
  }
}
